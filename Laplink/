[31m-        hours: hours,[m
[31m-        minutes: minutes,[m
[31m-        seconds: seconds,[m
[31m-        milliseconds: milliseconds,[m
[31m-      );[m
[32m+[m[32m          hours: hours,[m
[32m+[m[32m          minutes: minutes,[m
[32m+[m[32m          seconds: seconds,[m
[32m+[m[32m          milliseconds: milliseconds);[m
